<?xml version='1.0' encoding='utf-8'?>
<widget id="com.breakpoint.reelitin" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Reelitin</name>
    <description>
        The Cordova app for Reelitin
    </description>
    <author email="breakpoint.rcc@gmail.com" href="https://github.com/Sensrdt/an314_BreakPoint_SIH2020">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
</widget>
